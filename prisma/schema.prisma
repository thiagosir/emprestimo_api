// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Customer {
  id       Int    @id @default(dbgenerated())
  cpf      String @unique
  name     String
  age      Int
  income   Float
  location String
}

model Loan {
  id   Int    @id @default(dbgenerated())
  type String @unique
  rate Float
}
